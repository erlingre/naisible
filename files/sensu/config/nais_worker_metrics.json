{
  "checks": {
    "check_process_dockerd": {
      "handlers": [
        "metrics"
      ],
      "command": "/etc/sensu/plugins/metrics/nais/process-metrics.sh dockerd",
      "interval": 100,
      "standalone": true
    },
    "check_process_kubelet": {
      "handlers": [
        "metrics"
      ],
      "command": "/etc/sensu/plugins/metrics/nais/process-metrics.sh kubelet",
      "interval": 100,
      "standalone": true
    },
    "check_process_flanneld": {
      "handlers": [
        "metrics"
      ],
      "command": "/etc/sensu/plugins/metrics/nais/process-metrics.sh flanneld",
      "interval": 100,
      "standalone": true
    },
    "check_process_kube_proxy": {
      "handlers": [
        "metrics"
      ],
      "command": "/etc/sensu/plugins/metrics/nais/process-metrics.sh kube-proxy",
      "interval": 100,
      "standalone": true
    },
    "check_interface_docker": {
      "handlers": [
        "metrics"
      ],
      "command": "/etc/sensu/plugins/metrics/nais/interface-metrics.sh docker0",
      "interval": 100,
      "standalone": true
    },
    "check_interface_flannel.1": {
      "handlers": [
        "metrics"
      ],
      "command": "/etc/sensu/plugins/metrics/nais/interface-metrics.sh flannel.1",
      "interval": 100,
      "standalone": true
    }
  }
}
